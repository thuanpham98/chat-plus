{
  "name": "chat-plus",
  "version": "0.0.0",
  "private": true,
  "description": "chat app platfrom for muilty mini app",
  "author": {
    "name": "thuanpm",
    "email": "pmttmp24@gmail.com",
    "url": "https://github.com/thuanpham98"
  },
  "postinstall": "electron-builder install-app-deps",
  "scripts": {
    "dev:react": "cross-env NODE_ENV=development webpack serve --config webpack.react.js --mode=development",
    "build:react": "cross-env NODE_ENV=production webpack --config webpack.react.js --mode=production",
    "dev:electron": "rm -rf /dist/app && cross-env NODE_ENV=development webpack --config webpack.electron.js --mode=development && npm run start:electron",
    "build:electron": "rm -rf /dist/app && cross-env webpack --config webpack.electron.js --mode=production",
    "start:electron": "electron dist/app/main.js",
    "clean": "rm -rf chat-app-production",
    "dist": "npm run clean && cross-env NODE_ENV=production electron-builder",
    "dir": "cross-env NODE_ENV=production electron-builder --dir",
    "generate:message-protoc": "protoc --ts_out src/infrastructure/message-protobuf --proto_path src/infrastructure/message-protobuf src/infrastructure/message-protobuf/message.proto"
  },
  "dependencies": {
    "@protobuf-ts/protoc": "^2.9.4",
    "@radts/reactjs": "^2.1.4",
    "axios": "^1.6.8",
    "dotenv": "^16.4.5",
    "immutable": "^5.0.0-beta.5",
    "install": "^0.13.0",
    "npm": "^10.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.24.1",
    "@protobuf-ts/plugin": "^2.9.4",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20.12.4",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.23",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "dotenv-webpack": "^8.1.0",
    "electron": "^22.3.27",
    "electron-builder": "^24.13.3",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "npm-run-all": "4.1.5",
    "prettier": "^2.7.1",
    "serve": "^14.0.0",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.4",
    "wait-on": "^6.0.1",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2"
  }
}
